prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libalpm
Description: Arch Linux package management library
URL: http://www.archlinux.org/pacman/
Version: 8.0.2
Cflags: -I${includedir}
Libs: -L${libdir} -lalpm
Libs.private: -lm  -larchive -lnettle -llzo2 -llzma -lbz2 -lz -lxml2 -lz -lpthread -liconv -lm  -lcrypto -lz  -lcurl -lidn -lssh2 -lssl -lcrypto -lgdi32 -lssl -lcrypto -lz  -lgpgme -lassuan -lgpg-error
